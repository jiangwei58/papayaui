{
  "easycom": {
    "autoscan": true,
    "custom": {
      "^wei-(.*)": "@/packages/components/$1/$1.vue"
    }
  },
  "pages": [
    {
      "path": "pages/example/index",
      "style": {
        "navigationBarTitleText": "示例"
      }
    },
    {
      "path": "packages/components/container/demo",
      "style": {
        "navigationBarTitleText": "Container 页面容器"
      }
    },
    {
      "path": "packages/components/button/demo",
      "style": {
        "navigationBarTitleText": "Button 按钮"
      }
    },
    {
      "path": "packages/components/cascader/demo",
      "style": {
        "navigationBarTitleText": "Cascader 级联选择"
      }
    },
    {
      "path": "packages/components/picker-popup/demo",
      "style": {
        "navigationBarTitleText": "PickerPopup 选择弹窗"
      }
    },
    {
      "path": "packages/components/form/demo",
      "style": {
        "navigationBarTitleText": "Form 表单"
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "weiui",
    "navigationBarBackgroundColor": "#FFFFFF",
    "backgroundColor": "#F2F3F5",
    "usingComponents": {}
  }
}
